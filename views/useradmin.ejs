<%- include header %>
<% if (user) { %>
<%- include sider %>
<% } else { %>
<script>location.href = "/login"</script>
<% } %>
<div class="wid useradmin-page">
	<h4>中医体质辨识系统用户管理</h4>
	<table class="table table-striped table-bordered table-hover table-responsive useradmin-table">
		<thead>
		<th width="5%">编号</th>
		<th width="13%">姓名</th>
		<th width="10%">性别</th>
		<th width="10%">年龄</th>
		<th width="18%">联系方式</th>
		<th width="17%">身份证号</th>
		<th width="27%">操作</th>
		</thead>
		<tbody>
		<% questionone.forEach(function (questionone, index) { %>
		<tr>
			<td><%= index+1 %></td>
			<td><%= questionone.name %></td>
			<td><%= questionone.sex=="man"?"男":"" %><%= questionone.sex=="woman"?"女":"" %></td>
			<td><%= questionone.age %></td>
			<td><%= questionone.tel %></td>
			<td><%= questionone.idcard %></td>
			<td><button type="button" class="btn btn-default btn-xs" id="del-btn"><span class="glyphicon glyphicon-trash"></span>删除</button><button type="button" class="btn btn-primary btn-xs" id="report-view"><a href="/report-view"><span class="glyphicon glyphicon-eye-open"></span>在线查看报告</a></button><button type="button" class="btn btn-default btn-xs" id="report-download"><a href="/report-download"><span class="glyphicon glyphicon-download-alt"></span>下载报告</a></button></td>
		</tr>
		<% })%>

		</tbody>
	</table>
</div>
<%- include footer %>