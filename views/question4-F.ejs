	       <td><input type="radio" name="Fscore4" value="1" checked></td>
	       <td><input type="radio" name="Fscore4" value="2"></td>
	       <td><input type="radio" name="Fscore4" value="3"></td>
	       <td><input type="radio" name="Fscore4" value="4"></td>
	       <td><input type="radio" name="Fscore4" value="5"></td>
